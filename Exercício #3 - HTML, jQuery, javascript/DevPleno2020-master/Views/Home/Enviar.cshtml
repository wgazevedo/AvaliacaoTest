@{
    ViewData["Title"] = "Obrigado!";
}

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
    <br />
    
       @for (int i = 0; i < ViewBag.Filmes.Count; i++)
        {
            var filme = ViewBag.Filmes[i];

        <div class="row" id="filme_@i" style="margin-bottom: 10px;">
            <div class="col-md-12">
                <p>@(i+1) : @filme</p>
            </div>
        </div>

        }

</div>
